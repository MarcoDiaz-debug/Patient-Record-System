<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.image.Image?>

<BorderPane fx:controller="grprs.Controller" translateY='20' maxHeight="676" prefWidth="1366" fx:id="removedPatient" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" stylesheets="@styles.css">
    <left>
        <HBox fx:id="left">
            <StackPane fx:id="leftPane">
                <VBox fx:id="Menu" spacing="10">
                    <Label fx:id="HospitalNameTag" text="OSPA Leyte" alignment="CENTER" prefHeight="53" maxWidth="Infinity"/>
                    <Button prefHeight="53" maxWidth="Infinity" text="Patient History" onAction="#patientHistory"/>
                    <Button prefHeight="53" maxWidth="Infinity" text="Patient Record" onAction="#patientRecord"/>
                    <Button styleClass="active" prefHeight="53" maxWidth="Infinity" text="Removed Patient" onAction="#RemovedPatient"/>
                </VBox>
            </StackPane>
        </HBox>
    </left>
    <right>
        <HBox fx:id="right">
            <StackPane fx:id="rightPane">
            
            </StackPane>
        </HBox>
    </right>
</BorderPane>


